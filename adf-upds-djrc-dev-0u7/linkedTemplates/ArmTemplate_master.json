{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-upds-djrc-dev-0u7"
		},
		"FileLinkedService_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"FileLinkedService\""
		},
		"ADLS_LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"ADLS_LinkedService\""
		},
		"FileLinkedService_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\MSI\\C$\\Users\\Dania\\sharedknow"
		},
		"FileLinkedService_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "DANIA"
		},
		"ADLS_LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsupdsdjrcdev89o.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-upds-djrc-dev-0u7_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LinkedService_accountKey": {
						"value": "[parameters('ADLS_LinkedService_accountKey')]"
					},
					"FileLinkedService_password": {
						"value": "[parameters('FileLinkedService_password')]"
					},
					"ADLS_LinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LinkedService_properties_typeProperties_url')]"
					},
					"FileLinkedService_properties_typeProperties_host": {
						"value": "[parameters('FileLinkedService_properties_typeProperties_host')]"
					},
					"FileLinkedService_properties_typeProperties_userId": {
						"value": "[parameters('FileLinkedService_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}